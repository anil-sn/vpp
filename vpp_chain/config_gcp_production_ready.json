{
  "default_mode": "gcp_production",
  "description": "Corrected GCP production configuration for receiving and processing traffic from AWS.",
  "modes": {
    "gcp_production": {
      "description": "GCP IPsec decryption, NAT reversal, and forwarding to FDI with NAT IP 44.238.178.247",
      "networks": [
        {
          "name": "gcp-ingress-net",
          "subnet": "172.21.100.0/24",
          "gateway": "172.21.100.1",
          "description": "GCP traffic ingress from AWS",
          "mtu": 1500
        },
        {
          "name": "gcp-internal-net",
          "subnet": "172.21.101.0/24",
          "gateway": "172.21.101.1",
          "description": "Internal network for decrypted traffic"
        }
      ],
      "containers": {
        "security-processor": {
          "description": "IPsec decryption, defragmentation, and NAT reversal for GCP FDI",
          "dockerfile": "src/containers/Dockerfile.security",
          "config_script": "src/containers/security-config.sh",
          "interfaces": [
            {
              "name": "eth0",
              "network": "gcp-ingress-net",
              "ip": {
                "address": "172.21.100.10",
                "mask": 24
              }
            },
            {
              "name": "eth1",
              "network": "gcp-internal-net",
              "ip": {
                "address": "172.21.101.10",
                "mask": 24
              }
            }
          ],
          "ipsec": {
            "sa_in": {
              "id": 1000,
              "spi": 1000,
              "crypto_alg": "aes-gcm-128",
              "crypto_key": "fedcba9876543210abcdef0123456789"
            },
            "sa_out": {
              "id": 2000,
              "spi": 2000,
              "crypto_alg": "aes-gcm-128",
              "crypto_key": "a1b2c3d4e5f6789012345678abcdef01"
            },
            "tunnel": {
              "src": "34.134.82.101",
              "dst": "34.212.132.203",
              "local_ip": "10.100.100.2/30",
              "remote_ip": "10.100.100.1/30"
            }
          },
          "fragmentation": {
            "mtu": 1400,
            "enable": true
          },
          "nat44": {
            "sessions": 4096,
            "static_mapping": {
              "local_ip": "172.21.101.20",
              "local_port": 2055,
              "external_ip": "10.10.10.10",
              "external_port": 2055
            },
            "inside_interface": "eth1",
            "outside_interface": "eth0"
          },
          "routes": [
            {
              "to": "10.10.10.0/24",
              "via": "172.21.101.20",
              "interface": "eth1"
            },
            {
              "to": "34.212.132.203/32",
              "via": "172.21.100.1",
              "interface": "eth0"
            }
          ]
        },
        "destination": {
          "description": "GCP FDI service destination for decrypted traffic",
          "dockerfile": "src/containers/Dockerfile.destination",
          "config_script": "src/containers/destination-config.sh",
          "interfaces": [
            {
              "name": "eth0",
              "network": "gcp-internal-net",
              "ip": {
                "address": "172.21.101.20",
                "mask": 24
              }
            }
          ],
          "tap_interface": {
            "id": 0,
            "name": "vpp-tap0",
            "ip": "10.0.4.1/24",
            "linux_ip": "10.0.4.2/24",
            "pcap_file": "/tmp/gcp-fdi-processed.pcap",
            "rx_mode": "interrupt"
          },
          "fdi_forwarding": {
            "enabled": true,
            "fdi_service_ip": "10.0.4.100",
            "fdi_service_port": 8081,
            "preserve_source_ip": true,
            "nat_outbound_ip": "44.238.178.247"
          },
          "routes": [
            {
              "to": "0.0.0.0/0",
              "via": "172.21.101.1"
            },
            {
              "to": "10.0.4.0/24",
              "via": "tap0"
            }
          ]
        }
      },
      "connectivity_tests": [
        {
          "from": "security-processor",
          "to": "172.21.101.20",
          "description": "Security Processor -> Destination"
        }
      ],
      "traffic_config": {
        "expected_transformations": [
          "IPsec decryption (security-processor)",
          "NAT44 reverse translation (security-processor)",
          "FDI forwarding (destination)"
        ]
      }
    }
  }
}